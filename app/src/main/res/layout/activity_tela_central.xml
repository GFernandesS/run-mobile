<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.tela_central">


    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.tela_central" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="160dp"
        android:layout_height="75dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:background="@drawable/customizado1" />


    <Chronometer
        android:id="@+id/cronometroCorrida"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="145dp"
        android:layout_marginTop="22dp"
        android:textSize="30dip"
        />

    <Button
        android:id="@+id/btnIniciar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="585dp"
        android:background="@drawable/customizado1"
        android:layout_width="270dp"
        android:onClick="onClickIniciar"
        android:layout_height="60dp"
        android:text="INICIAR" />

    <ImageView
        android:id="@+id/imgHistorico"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="18dp"
        android:layout_toRightOf="@id/imageView"
        android:onClick="onClickHistorico"
        app:srcCompat="@drawable/ic_assignment_black_24dp" />


</RelativeLayout>